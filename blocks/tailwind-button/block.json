{
    "apiVersion": 2,
    "name": "enhanced-tailwind-wp/button",
    "title": "Tailwind Button",
    "category": "design",
    "icon": "button",
    "description": "A button with Tailwind CSS classes",
    "supports": {
        "html": false
    },
    "textdomain": "enhanced-tailwind-wp",
    "attributes": {
        "text": {
            "type": "string",
            "default": "Click Me"
        },
        "url": {
            "type": "string",
            "default": "#"
        },
        "className": {
            "type": "string",
            "default": "bg-blue-500 hover:bg-blue-700 text-white font-bold py-2 px-4 rounded"
        },
        "target": {
            "type": "string",
            "default": "_self"
        }
    },
    "editorScript": "file:./index.js",
    "style": "file:./style.css"
}